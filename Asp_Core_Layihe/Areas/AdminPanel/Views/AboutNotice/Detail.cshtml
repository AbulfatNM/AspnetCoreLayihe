@model AboutNotice
@{
    ViewData["Title"] = "Detail";
}


<div class="container">
 
    <div class="text-center mt-3">
        <h4>Date</h4>
        <p>@Model.Date </p>
        <h4 style="margin: 10px 0px;">Description</h4>
        <p>@Model.Description</p>
    </div>
    <div class="row mt-4">
        <a class="btn btn-dark text-white" asp-controller="AboutNotice" asp-action="Index">Go Back</a>
    </div>
</div>

