@model AboutWelcome
@{
    ViewData["Title"] = "Detail";
}

<div class="container">
    <div class="row d-flex justify-content-center">
        <img style="width:450px; height:250px" src="~/img/about/@Model.Image" />

    </div>
    <div class="text-center mt-3">
        <h5>Title</h5>
        <p>@Model.Title </p>
        <h5 class="mt-3">Description</h5>
        <p>@Model.Description</p>
    </div>
    <div class="row mt-4">
        <a class="btn btn-dark text-white" asp-controller="AboutWelcome" asp-action="Index">Go Back</a>
    </div>
</div>


