@model Speaker
@{
    ViewData["Title"] = "Detail";
}

    <div class="container">
        <div class="row d-flex justify-content-center">
            <img style="width:450px; height:250px" src="~/img/event/@Model.Image" />

        </div>
        <div class="text-center mt-3">
            <h5>Full Name</h5>
            <p>@Model.FullName </p>
            <h5 class="mt-3">Position</h5>
            <p>@Model.Position  </p>
        </div>
        <div class="row mt-4">
            <a class="btn btn-dark text-white" asp-controller="Speaker" asp-action="Index">Go Back</a>
        </div>
    </div>

